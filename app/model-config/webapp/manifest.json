{
	"_version": "1.8.0",
	"sap.app": {
		"id": "model-config",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "0.0.1"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"resources": "resources.json",
		"dataSources": {
			"ConfigService": {
				"uri": "/api/ConfigService/",
				"type": "OData",
				"settings": {
					"odataVersion": "4.0"
				}
			}
		}
	},
	"sap.ui5": {
		"dependencies": {
			"libs": {
				"sap.fe.templates": {}
			},
			"minUI5Version": "1.122.1"
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "model-config.i18n.i18n"
				}
			},
			"": {
				"dataSource": "ConfigService",
				"preload": true,
				"settings": {
					"synchronizationMode": "None",
					"operationMode": "Server",
					"autoExpandSelect": true,
					"earlyRequests": true,
					"groupProperties": {
						"default": {
							"submit": "Auto"
						}
					}
				}
			},
			"@i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/i18n.properties"
			}
		},
		"routing": {
			"routes": [
				{
					"pattern": ":?query:",
					"name": "ModelConfigList",
					"target": "ModelConfigList"
				},
				{
					"pattern": "ModelConfigs({key}):?query:",
					"name": "ModelConfigObjectPage",
					"target": "ModelConfigObjectPage"
				}
			],
			"targets": {
				"ModelConfigList": {
					"type": "Component",
					"id": "ModelConfigList",
					"name": "sap.fe.templates.ListReport",
					"options": {
						"settings": {
							"entitySet": "ModelConfigs",
							"navigation": {
								"ModelConfigs": {
									"detail": {
										"route": "ModelConfigObjectPage"
									}
								}
							},
							"initialLoad": "Enabled"
						}
					}
				},
				"ModelConfigObjectPage": {
					"type": "Component",
					"id": "ModelConfigObjectPage",
					"name": "sap.fe.templates.ObjectPage",
					"options": {
						"settings": {
							"contextPath": "/ModelConfigs",
							"navigation": {},
							"content": {
								"header": {
									"anchorBarVisible": true
								}
							},
							"editableHeaderContent": false,
							"sectionLayout": "Page"
						}
					}
				}
			},
			"config": {}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"fullWidth": false,
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.fiori": {}
}
